@{
    ViewData["Title"] = "Numbers From 1 To 50";
}


<div class="row">
    <div class="col-md-4">
        <h2>Numbers:</h2>
        <ul class="list-group">
            @for (int i = 1; i <= 50; i++)
            {
                <li class="list-group-item">@i</li>
            }
        </ul>
    </div>

</div>